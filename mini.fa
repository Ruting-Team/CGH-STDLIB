# template of character
type: int
# alphabet
sigma: 1 2 3
# number of states, coding with id (0..n-1)
states: 5
# initial state id
initial: 0
# final states
final: 3 4
# transitions order by state id
# each transition like: src_id character dst_id
# example: (1 1* 3) | (1 1* 2+ 3+)
transitions:
0 1 1
1 1 1
1 2 2
1 3 4
2 2 2
2 3 3
3 3 3

